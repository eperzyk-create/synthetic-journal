services:
  - type: web
    name: tsj-api
    env: python
    plan: free
    buildCommand: pip install -r requirements.txt
    startCommand: uvicorn main:app --host 0.0.0.0 --port $PORT
    autoDeploy: true
    envVars:
      - key: TSJ_DB_PATH
        value: tsj.db
      - key: TSJ_SCHEMA_PATH
        value: tsj_schema.json
